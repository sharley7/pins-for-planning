

<%= form_for @user do | f | %>

<%= render '/layouts/user_form_errors', locals: {user: @user} %>

<%= f.label :username %>
<%= f.text_field :username %>
<br />
<%= f.label :email %>
<%= f.text_field :email %>
<br />
<%= f.label :password %>
<%= f.password_field :password %>
<br />
<%= f.submit %>

<% end %>
