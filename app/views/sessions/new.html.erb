<%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>

<%= render '/layouts/user_form_errors', locals: {user: @user} %>

<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.submit "Sign in" %>
<% end %>
